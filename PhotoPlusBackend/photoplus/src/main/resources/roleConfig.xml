<!--
This file contains role authenticated URL's configuration.
It is required for proper system work.
Use it wisely!
Proper file format example:
<config>
    <role name="[PUT_ROLE_HERE]">
        <access>[PUT_ALLOWED_URL_HERE]</access>
        <access>[PUT_ALLOWED_URL_HERE]</access>
    </role>
     <role name="[PUT_ROLE_HERE]">
        <access>[PUT_ALLOWED_URL_HERE]</access>
        <access>[PUT_ALLOWED_URL_HERE]</access>
         <access>[PUT_ALLOWED_URL_HERE]</access>
    </role>
