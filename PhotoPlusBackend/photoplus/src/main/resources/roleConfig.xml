<config>
    <role name="client">
        <access>CLIENT_ROLE</access>
    </role>
    <role name="admin">
        <access>ADMIN_ROLE</access>
        <access>/user/all</access>
        <access>/user/patch</access>
        <access>/user/delete</access>
        <access>/user/post</access>
    </role>
</config>